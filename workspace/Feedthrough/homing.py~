from FeedthroughMotorControls import *
import os

initFeedtrough()

MonitorAxis = 2

while (1):
  try:
    os.system('clear')
    MonitorParameter(MonitorAxis,"I1")
    MonitorParameter(MonitorAxis,"I2")
    MonitorParameter(MonitorAxis,"I3")
    MonitorParameter(MonitorAxis,"I4")
    
    time.sleep(0.5)
  except KeyboardInterrupt:
    print "Bye"
    sys.exit()



